{
  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "version": "1.6",
  "description": "Synchronization, search and organization folders for Gemini. Author: AinsTitor",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content_loader.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.js", "storage.js", "ui.js", "main.js"],
      "matches": ["https://gemini.google.com/*"]
    }
  ]
}
