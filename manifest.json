{
  "manifest_version": 3,
  "name": "Gemini Ultimate Organizer",
  "version": "2.0",
  "description": "Gemini Ultimate Organizer is a Chrome extension specializing in file organization, prompt folder, streamer mode, wide mode etc. offering several interesting features.",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content_loader.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.js", "storage.js", "ui.js", "main.js", "i18n.js"],
      "matches": ["https://gemini.google.com/*"]
    }
  ]
}
